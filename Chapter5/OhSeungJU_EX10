public class OhSeungJU_EX10 : MonoBehaviour
{
    // Start is called before the first frame update
    void Start()
    {
        for (int i = 0; i < 5; ++i)
        {
            string sentence = "";

            for (int x = 0; x <= i; ++x)
            {
                sentence += "★";
            }

            for (int x = i + 1; x < 5; ++x)
            {
                sentence += "☆";
            }
            Debug.Log(sentence);
        }

    }
}
